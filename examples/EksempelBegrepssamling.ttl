@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skosxl: <http://www.w3.org/2008/05/skos-xl#> .
@prefix xkos: <http://rdf-vocabulary.ddialliance.org/xkos#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix skosno: <https://data.norge.no/vocabulary/skosno#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix : <https://examples.com/begreper/> .

# Dette eksemplet på en Begrepssamling skal valideres OK (uten violation/warning)
<https://examples.com/begrepssamling/ex1somSkalVæreOK>
  a skos:Collection;  # Begrepssamling
  # --- mandatory ---
  rdfs:label "Eksempelsamling"@nb ; # navn
  dct:identifier <https://examples.com/begrepssamling/ex1IngenFeil> ; # identifikator
  dct:publisher <https://data.brreg.no/enhetsregisteret/api/enheter/991825827> ; # ansvarligVirksomhet
  # --- optional ---
  dct:description "Et eksempel på en begrepssamling"@nb ;
  dcat:concactPoint [ a vcard:organization ; # kontaktpunkt
    vcard:organizationUnit "Avdeling for eksemplifisering" ;
    vcard:hasEmail <mailto:examples@examples.com> ;
    ] ;
  skos:member <https://examples.com/begreper/ex2somSkalVæreOK> ;
  .

# Dette eksemplet på et Begrep tas fordi en samling må ha minst 1 begrep.
# EKsempelet skal ikke gi valideringsfeil
<https://examples.com/begreper/ex2somSkalVæreOK>
  a skos:Concept;  # Begrep
  # --- mandatory ---
  skosxl:prefLabel [ # term
    a skosxl:Label ; # AnbefaltTerm
    skosxl:literalForm "eksempelbegrep"@nb ; # navn
  ] ;
  skosno:betydningsbeskrivelse [  # betydningsbeskrivelse
    a skosno:Definisjon ; # Definisjon
    rdfs:label "eksempel på en begrepsbeskrivelse"@nb ; # tekst
    dct:source [ # kildebeskrivelse
     rdfs:label "Eksempelordboka"@en ; # tekst
     ] ;
    ] ;
  dct:publisher <https://data.brreg.no/enhetsregisteret/api/enheter/991825827> ; # ansvarligVirksomhet
  dct:identifier <https://examples.com/begreper/ex1IngenFeil> ; # identifikator
.
