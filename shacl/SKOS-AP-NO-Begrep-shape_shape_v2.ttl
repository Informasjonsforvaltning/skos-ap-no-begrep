##### -------------------- Class shapes, with some special property shapes

### Klassen Begrep (skos:Concept)
:Concept_Shape a sh:NodeShape;
   rdfs:label "Concept"@en, "Begrep"@nb;
   # mandatory properties:
      # preferred term
         sh:property :hasMin2MaxNShape-skosPrefLabel; # anbefalt term, minimum 2 (skos:prefLabel) og typesjekk
         sh:property :uniqueLangShape-skosPrefLabel; # maks. 1 per språk (skos:prefLabel)
         sh:property :hasMin1Max1Shape-skosPrefLabelNynorsk; # min. 1 og maks. 1 prefLabel på Nynorsk (skos:prefLabel)
         sh:property :hasMin1Max1Shape-skosPrefLabelBokmål;  # min. 1 og maks. 1 prefLabel på Bokmål (skos:prefLabel)
      # definition
         sh:property :UniqueLangShape-skosDefinition; # definisjon, maks. 1 per språk (skos:definition)
         sh:property :Min1MaxNShape-EitherSkosDefinitionOrEuvocXlDefinition; # min. 1 definisjon, enten skos:definition eller euvoc:xlDefinition
         sh:property :hasMin0MaxNShape-euvocXlDefinition; # definisjon (euvoc:xlDefinition), sjekk klassetilhørighet
      # other mandatory
         sh:property :hasMin1MaxNShape-dctIdentifier; # identifikator (dct:identifier)
         sh:property :hasMin1MaxNShape-dcatContactPoint; # kontaktpunkt (dcat:contactPoint)
         sh:property :hasMin1Max1Shape-dctPublisher; # publisert av (dct:publisher)
   # recommended:
         sh:property :hasMin0Max1Shape-dctCreator; # ansvarlig virksomhet (dct:creator)
         sh:property :hasMin0Max1Shape-euvocStartDate; # dato gyldig fra og med (euvoc:startDate)
         sh:property :hasMin0Max1Shape-euvocEndDate; # dato gyldig til og med (euvoc:endDate)
         sh:property :hasMin0Max1Shape-dctCreated; # dato opprettet (dct:created)
         sh:property :hasMin0Max1Shape-dctModified; # dato sist oppdatert (dcat:modified)
         sh:property :hasMin0MaxNShape-dctSubject; # fagområde (dct:subject)
         sh:property :hasMin0MaxNShape-skosScopeNote; # merknad (skos:scopeNote)
         sh:property :hasMin0MaxNShape-skosAltLabel; # tillatt term (skos:altLabel)
   # optional:
         sh:property :hasMin0MaxNShape-skosnoDataStructureLabel; # datastrukturterm (skosno:dataStructureLabel)
         sh:property :hasMin0MaxNShape-skosExample; # eksempel (skos:example)
         sh:property :hasMin0MaxNShape-xkosIsPartOf; # er del av (xkos:isPartOf)
         sh:property :hasMin0MaxNShape-dctIsReplacedBy; # er erstattet av (dct:isReplacedBy)
         sh:property :hasMin0MaxNShape-skosnoIsFromConceptIn; # er fra-begrep i (skosno:isFromConceptIn)
         sh:property :hasMin0MaxNShape-skosRelated; # Begrep – er relatert til (skos:related)
         sh:property :hasMin0MaxNShape-dctReplaces; # erstatter (dct:replaces)
         sh:property :hasMin0MaxNShape-skosHiddenLabel; # frarådd term (skos:hiddenLabel)
         sh:property :hasMin0MaxNShape-xkosGeneralizes; # generaliserer (xkos:generalizes)
         sh:property :hasMin0MaxNShape-skosExactMatch; # har eksakt samsvar med (skos:exactMatch)
         sh:property :hasMin0MaxNShape-skosnoHasGenericConceptRelation; # har generisk begrepsrelasjon (skosno:hasGenericConceptRelation)
         sh:property :hasMin0MaxNShape-skosCloseMatch; # har nært samsvar med (skos:closeMatch)
         sh:property :hasMin0MaxNShape-skosnoHasPartitiveConceptRelation; # har partitiv begrepsrelasjon (skosno:hasPartitiveConceptRelation)
         sh:property :hasMin0MaxNShape-xkosHasPart; # inneholder (xkos:hasPart)
         sh:property :hasMin0MaxNShape-uneskosMemberOf; # inngår i begrepssamling (uneskos:memberOf)
         sh:property :hasMin0MaxNShape-rdfsSeeAlso; # se også (rdfs:seeAlso)
         sh:property :hasMin0MaxNShape-xkosSpecializes; # spesialiserer (xkos:specializes)
         sh:property :hasMin0MaxNShape-euvocStatusEitherCodedValueOrText; # status (euvoc:status), enten kodet verdi eller tekst
         sh:property :hasMin0MaxNShape-skosnoValueRange; # verdiområde (skosno:valueRange)
         sh:property :hasMin0Max1Shape-owlVersionInfo; # versjonsnummer (owl:versionInfo)
         sh:property :hasMin0MaxNShape-admsVersionNotes; # versjonsnote (adms:versionNotes)
         ;
      sh:targetClass skos:Concept ; .

# --- PropertyShapes specific for skos:prefLabel 

:uniqueLangShape-skosPrefLabel a sh:PropertyShape;
   sh:name "preferred term"@en, "anbefalt term"@nb;
   sh:path skos:prefLabel;
   sh:datatype rdf:langString;
   sh:uniqueLang true;
   sh:message "The property skos:prefLabel may have max. 1 value per language."@en,
      "Egenskapen skos:prefLabel kan ha maks. 1 verdi per språk."@nb;
   sh:severity sh:Violation .

:hasMin1Max1Shape-skosPrefLabelBokmål a sh:PropertyShape;
   sh:name "preferred term"@en, "anbefalt term"@nb;
   sh:path skos:prefLabel;
   sh:datatype rdf:langString;
   sh:qualifiedValueShape [sh:languageIn ("nb")];
   sh:qualifiedValueShapesDisjoint true ;
   sh:qualifiedMinCount 1 ;
   sh:qualifiedMaxCount 1 ;
   sh:message "The property skos:prefLabel SHALL have min. 1 and max. 1 value with lang-tag 'nb'."@en,
      "Egenskapen skos:prefLabel SKAL ha min. 1 og maks. 1 verdi som har språkkode 'nb'."@nb;
   sh:severity sh:Violation .

:hasMin1Max1Shape-skosPrefLabelNynorsk a sh:PropertyShape;
   sh:name "preferred term"@en, "anbefalt term"@nb;
   sh:path skos:prefLabel;
   sh:datatype rdf:langString;
   sh:qualifiedValueShape [sh:languageIn ("nn")];
   sh:qualifiedValueShapesDisjoint true ;
   sh:qualifiedMinCount 1 ;
   sh:qualifiedMaxCount 1 ;
   sh:message "The property skos:prefLabel SHALL have min. 1 and max. 1 value which has lang-tag 'nn'."@en,
      "Egenskapen skos:prefLabel SKAL ha min. 1 og maks. 1 verdi som har språkkode 'nn'."@nb;
   sh:severity sh:Violation .

# - PropertyShapes for specific for skos:definition and/or euvoc:xlDefinition: 

:UniqueLangShape-skosDefinition a sh:PropertyShape;
   sh:name "definisjon (direkte angivelse) (skos:definition)"@en,
      "definition (direct statement) (skos:definition)"@nb;
   sh:path skos:definition;
   sh:uniqueLang true;
   sh:message "The property skos:definition may have max. 1 value per language"@en,
      "Egenskapen skos:definition kan ha maks. 1 verdi per språk"@nb;
   sh:severity sh:Violation .

 :Min1MaxNShape-EitherSkosDefinitionOrEuvocXlDefinition a sh:PropertyShape;
   sh:name "Either skos:definition or eucov:xlDefinition SHALL have min. 1 value"@en,
      "Enten skos:definition eller eucov:xlDefinition SKAL ha min. 1 verdi"@en;
   sh:path [sh:alternativePath (skos:definition euvoc:xlDefinition)];
   sh:minCount 1;
   sh:message "Either skos:definition or euvoc:xlDefinition SHALL have min. 1 value"@en,
      "Enten skos:definition eller euvoc:xlDefinition SKAL ha min. 1 verdi"@nb;
   sh:severity sh:Violation .

### Klassen Definisjon (euvoc:XlNote)
:XlNote-Shape a sh:NodeShape;
   rdfs:label "Definition"@en, "Definisjon"@nb;
   sh:property
      # mandatory
         :hasMin1Max1Shape-rdfValue, # tekst (rdf:value)
      # recommended
         :hasMin0Max1Shape-skosnoRelationshipWithSource, # forhold til kilde (skosno:relationshipWithSource)
         :CVShape-skosnoRelationshipWithSource, # forhold til kilde (skosno:relationshipWithSource), kontrollert vokabular
         :hasMin0MaxNShape-dctSource, # kilde (dct:source)
      # optional
         :hasMin0Max1Shape-dctAudience, # målgruppe (dct:audience)
         :CVShape-dctAudience # målgruppe (dct:audience) kontrollert vokabular
      ;
   sh:targetClass euvoc:XlNote ; .

### Klassen Assosiativ begrepsrelasjon (skosno:AssociativeConceptRelation)
:AssociativeConceptRelation-Shape1 a sh:NodeShape; # til-begrep
   rdfs:label "Associative concept relation"@en, "Assosiativ begrepsrelasjon"@nb;
   # mandatory
      sh:property :hasMin1MaxNShape-skosnoHasToConcept; # har til-begrep (skosno:hasToConcep)
   sh:targetClass skosno:AssociativeConceptRelation .

:AssociativeConceptRelation-Shape2 a sh:NodeShape; # relasjonsrolle - min 1, enten eller
   rdfs:label "Associative concept relation"@en, "Assosiativ begrepsrelasjon"@nb;
   # mandatory
      sh:xone ( [sh:property [
            sh:name "relation role - as coded value"@en, "relasjonsrolle - som kodet verdi"@nb;
            sh:path skosno:relationRole;
            sh:class skos:Concept; 
            sh:minCount 1; ]
            ]
         [sh:property [
            sh:name "relation role - as text"@en, "relasjonsrolle - som tekst"@nb;
            sh:path skosno:relationRole;
            sh:datatype rdf:langString;
            sh:minCount 1; 
            ] 
            ]
      );
      sh:message "The property skosno:relationRole SHALL have min. 1 value, which is of datatype rdf:langString or which refers to an instance of skos:Concept, not both"@en,
         "Egenskapen skosno:relationRole skal ha min. 1 verdi, som enten er av datatype rdf:langString eller refererer til en instans av skos:Concept, ikke begge"@nb;
      sh:severity sh:Violation;
   sh:targetClass skosno:AssociativeConceptRelation .

# følgende 2 shapes virker ikke ennå, derfor kommentert bort
