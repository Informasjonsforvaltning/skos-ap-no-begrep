### Klassen Generisk begrepsrelasjon (skosno:GenericConceptRelation)
:GenericConceptRelation-Shape1 a sh:NodeShape; # inndelingskriterium
   rdfs:label "Generic concept relation"@en, "Generisk begrepsrelasjon"@nb;
   # recommended
      sh:property :hasMin0MaxNShape-dctDescriptionCriterionOfSubdivision; # inndelingskriterium (dct:description)
      sh:property :uniqueLangShape-dctDescriptionCriterionOfSubdivision; # inndelingskriterieum (dct:description), maks. 1 per språk
   sh:targetClass skosno:GenericConceptRelation .

:GenericConceptRelation-Shape2 a sh:NodeShape; # enten Generic eller Specific
   rdfs:label "Generic concept relation"@en, "Generisk begrepsrelasjon"@nb;
   # mandatory
   sh:xone (
      [sh:property [
         sh:path skosno:hasGenericConcept;
         sh:minCount 1; ] ]
      [sh:property [
         sh:path skosno:hasSpecificConcept;
         sh:minCount 1; ] ]
      );
   sh:message "Either skosno:hasGenericConcept or skosno:hasSpecificConcept, not both, SHALL have min. 1 value"@en,
      "Enten skosno:hasGenericConcept eller skosno:hasSpecificConcept, ikke begge, SKAL ha min. 1 verdi"@nb;
   sh:severity sh:Violation ;
   sh:targetClass skosno:GenericConceptRelation .

:GenericConceptRelation-Shape3 a sh:NodeShape; # klassetilhørighet
   rdfs:label "Generic concept relation"@en, "Generisk begrepsrelasjon"@nb;
   # mandatory
   sh:property [
      sh:path skosno:hasGenericConcept;
      sh:class skos:Concept;
      sh:message "The property skosno:hasGenericConcept SHALL refer to an instance of skos:Concept"@en, 
         "Egenskapen skosno:hasGenericConcept SKAL referere til en instans av skos:Concept"@nb; 
      ] ;
   sh:property [
      sh:path skosno:hasSpecificConcept;
      sh:class skos:Concept;
      sh:message "The property skosno:hasSpecificConcept SHALL refer to an instance of skos:Concept"@en, 
         "Egenskapen skosno:hasSpecificConcept SKAL referere til en instans av skos:Concept"@nb; 
      ] ;
   sh:severity sh:Violation ;
   sh:targetClass skosno:GenericConceptRelation .

   

### Klassen Partitiv begrepsrelasjon (skosno:PartitiveConceptRelation)
:PartitiveConceptRelation-Shape1 a sh:NodeShape; # inndelingskriterium
   rdfs:label "Partitive concept relation"@en, "Partitiv begrepsrelasjon"@nb;
   # recommended
      sh:property :hasMin0MaxNShape-dctDescriptionCriterionOfSubdivision; # inndelingskriterium (dct:description)
      sh:property :uniqueLangShape-dctDescriptionCriterionOfSubdivision; # inndelingskriterieum (dct:description), maks. 1 per språk
   sh:targetClass skosno:PartitiveConceptRelation .

:PartitiveConceptRelation-Shape2 a sh:NodeShape; # enten Comprehensive eller Partitive
   rdfs:label "Partitive concept relation"@en, "Partitiv begrepsrelasjon"@nb;
   # mandatory
      sh:xone (
         [sh:property [
            sh:path skosno:hasPartitiveConcept;
            sh:minCount 1; ] ]
         [sh:property [
            sh:path skosno:hasComprehensiveConcept;
            sh:minCount 1; ] ]
         );
   sh:message "Either skosno:hasComprehensiveConcept or skosno:hasPartitiveConcept, not both, SHALL have min. 1 value"@en,
      "Enten skosno:hasComprehensiveConcept eller skosno:hasPartitiveConcept, ikke begge, SKAL ha min. 1 verdi"@nb;
   sh:severity sh:Violation ;
   sh:targetClass skosno:PartitiveConceptRelation .

:PartitiveConceptRelation-Shape3 a sh:NodeShape; # klassetilhørighet
   rdfs:label "Partitive concept relation"@en, "Partitiv begrepsrelasjon"@nb;
   # mandatory
   sh:property [
      sh:path skosno:hasPartitiveConcept;
      sh:class skos:Concept;
      sh:message "The property skosno:hasPartitiveConcept SHALL refer to an instance of skos:Concept"@en, 
         "Egenskapen skosno:hasPartitiveConcept SKAL referere til en instans av skos:Concept"@nb; 
      ] ;
   sh:property [
      sh:path skosno:hasComprehensiveConcept;
      sh:class skos:Concept;
      sh:message "The property skosno:hasComprehensiveConcept SHALL refer to an instance of skos:Concept"@en, 
         "Egenskapen skosno:hasComprehensiveConcept SKAL referere til en instans av skos:Concept"@nb; 
      ] ;
   sh:severity sh:Violation ;
   sh:targetClass skosno:PartitiveConceptRelation .

### Klassen Begrepssamling (skos:Collection)
:Collection-Shape a sh:NodeShape;
   rdfs:label "Collection"@en, "Begrepssamling"@nb;
   # mandatory
      sh:property :hasMin1Max1Shape-dctIdentifier; # identifikator (dct:identifier)
      sh:property :hasMin1MaxNShape-skosMember; # inneholder begrep (skos:member)
      sh:property :hasMin1MaxNShape-dcatContactPoint; # kontaktpunkt (dcat:contactPoint)
      sh:property :hasMin1MaxNShape-dctTitle; # navn (dct:title)
      sh:property :hasMin1Max1Shape-dctPublisher; # publisert av (dct:publisher)
   # recommended
      sh:property :hasMin0MaxNShape-dctDescription; # beskrivelse (dct:description)
   sh:targetClass skos:Collection .

##### -------------------- general/reusable property shapes

##### adms-properties

:hasMin0MaxNShape-euvocStatusEitherCodedValueOrText a sh:PropertyShape;
   sh:name "status (as coded value or text)"@en, "status (som kodet verdi eller tekst)"@nb;
   sh:path euvoc:status;
   sh:or ( [sh:class skos:Concept] [sh:datatype rdf:langString]);
   sh:message "The property euvoc:status SHALL either refer to an instance of skos:Concept or be of datatype rdf:langString"@en, 
      "Egenskapen euvoc:status skal enten referere til en instans av skos:Concept eller være av datatype rdf:langSting"@nb; 
   sh:severity sh:Violation .

:hasMin0Max1Shape-euvocStatusCodedValue a sh:PropertyShape;
   sh:name "status (as coded value)"@en, "status (som kodet verdi)"@nb;
   sh:path euvoc:status;
   sh:qualifiedValueShape [sh:class skos:Concept;];
   sh:qualifiedValueShapesDisjoint true ;
   sh:qualifiedMinCount 0 ;
   sh:qualifiedMaxCount 1 ;
   sh:message "The property euvoc:status, as coded value, may have max. 1 value which SHALL refer to an instance of skos:Concept"@en,
      "Egenskapen euvoc:status, som kodet verdi, kan ha maks. 1 verdi som SKAL referere til en instans av skos:Concept"@nb;
   sh:severity sh:Violation .

:hasMin0MaxNShape-euvocStatusTextUnigueLang a sh:PropertyShape;
   sh:name "status (as text)"@en, "status (som tekst)"@nb;
   sh:path euvoc:status;
   sh:qualifiedValueShape [sh:datatype rdf:langString;];
   sh:uniqueLang true;
   sh:qualifiedValueShapesDisjoint true ;
   sh:qualifiedMinCount 0 ;
   sh:message "The property euvoc:status, as text, SHALL be of datatype rdf:langString, and may have max. 1 per language"@en,
      "Egenskapen euvoc:status, som tekst, SKAL være av datatype rdf:langString, og kan ha maks. 1 per språk"@nb;
   sh:severity sh:Violation .

:hasMin0MaxNShape-admsVersionNotes a sh:PropertyShape;
   sh:name "version note"@en, "versjonsnote"@nb;
   sh:path adms:versionNotes;
   sh:datatype rdf:langString;
   sh:message "The property adms:versionNotes SHALL be of datatype rdf:langString"@en,
      "Egenskapen adms:versionNotes SKAL være av datatype rdf:langString"@nb;
   sh:severity sh:Violation .

##### dcat-properties

:hasMin1MaxNShape-dcatContactPoint a sh:PropertyShape;
   sh:name "contact point"@en, "kontaktpunkt"@nb;
   sh:path dcat:contactPoint;
   sh:or ( [sh:class vcard:Organization] [sh:class vcard:Group]);
   sh:minCount 1;
   sh:message "The property dcat:contactPoint SHALL have min. 1 value which SHALL refer to an instance of vcard:Organization or vcard:Group"@en,
      "Egenskapen dcat:contactPoint SKAL ha min. 1 verdi som SKAL referere til en instans av vcard:Organization eller vcard:Group"@nb;
   sh:severity sh:Violation .

##### dct-properties

:hasMin0Max1Shape-dctAudience a sh:PropertyShape;
   sh:name "audience"@en, "målgruppe"@nb;
   sh:path dct:audience;
   sh:class skos:Concept;
   sh:maxCount 1;
   sh:message "The property dct:audience may have max. 1 value which SHALL refer to an instance of skos:Concept"@en,
      "Egenskapen dct:audience kan ha maks. 1 verdi som SKAL referere til en instans av skos:Concept"@nb;
   sh:severity sh:Violation .

:CVShape-dctAudience a sh:PropertyShape;
   sh:name "audience"@en, "målgruppe"@nb;
   sh:path dct:audience;
   sh:pattern "^https://data.norge.no/vocabulary/audience-type#";
   sh:message "The property skosno:relationshipWithSource SHOULD have value chosen from https://data.norge.no/vocabulary/audience-type"@en,
         "Egenskapen skosno:relationshipWithSource BØR ha verdi som er hentet fra https://data.norge.no/vocabulary/audience-type"@nb;
   sh:severity sh:Warning .

:hasMin0Max1Shape-dctCreated a sh:PropertyShape;
   sh:name "date created"@en, "dato opprettet"@nb;
   sh:path dct:created;
   sh:datatype xsd:date;
